{
    "compilerOptions":  {
                            "target":  "es5",
                            "lib":  [
                                        "dom",
                                        "dom.iterable",
                                        "esnext"
                                    ],
                            "allowJs":  false,
                            "skipLibCheck":  true,
                            "strict":  true,
                            "noEmit":  true,
                            "module":  "esnext",
                            "moduleResolution":  "node",
                            "resolveJsonModule":  true,
                            "isolatedModules":  true,
                            "jsx":  "react-jsx",
                            "esModuleInterop":  true,
                            "baseUrl":  ".",
                            "paths":  {
                                          "@/*":  [
                                                      "src/*"
                                                  ]
                                      },
                            "incremental":  true,
                            "plugins":  [
                                            {
                                                "name":  "next"
                                            }
                                        ],
                            "strictNullChecks":  true
                        },
    "include":  [
                    "next-env.d.ts",
                    "**/*.ts",
                    "**/*.tsx",
                    ".next/types/**/*.ts",
                    ".next/dev/types/**/*.ts"
                ],
    "exclude":  [
                    "node_modules",
                    "_archive_app__broken",
                    "src/app__broken",
                    ".next"
                ]
}

